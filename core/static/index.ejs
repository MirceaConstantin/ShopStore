lete": {"tags": ["developers"], "summary": "delete a product", "description": "You can get a product from the app with this method\n", "produces": ["application/json"], "parameters": [], "responses": {"200": {"description": "products list", "schema": {"type": "array", "items": {"$ref": "#/definitions/Product"}}}, "404": {"description": "Page not found"}}}}}, "definitions": {"Product": {"type": "object", "required": ["description", "genre", "id", "imagePoster", "imageSlider", "platform", "price", "stock", "title"], "properties": {"id": {"type": "integer", "example": 1}, "title": {"type": "string", "example": "Fifa 20"}, "imagePoster": {"type": "string", "format": "url", "example": "https://xboxsquad.fr/wp-content/uploads/2019/07/Zidane-Fifa-20-Ultimate-e1564492804481.jpg"}, "imageSlider": {"type": "array", "format": "url", "example": ""}, "trilerGame": {"type": "string", "format": "url", "example": "https://www.youtube.com/watch?v=vgQNOIhRsV4"}, "description": {"type": "string", "example": "Description"}, "price": {"type": "integer", "example": 50}, "genre": {"type": "array", "example": ""}, "platform": {"type": "array", "example": ""}, "stock": {"type": "integer", "example": 200}}}}};
